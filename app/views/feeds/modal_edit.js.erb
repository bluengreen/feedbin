<% if @subscription %>
    feedbin.modalContent("<%= j render partial: 'settings/subscriptions/form', locals: {subscription: @subscription, unsubscribe: link_to('Unsubscribe', feed_destroy_subscription_path(@subscription.feed), method: :delete, remote: true, class: 'button-secondary pull-right settings-unsubscribe' , data: { confirm: 'Are you sure you want to unsubscribe?' })} %>");
    feedbin.drawBarCharts();
<% end %>
